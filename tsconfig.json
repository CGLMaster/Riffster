{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/astro*": ["src/components/astro/*"],
      "@/react*": ["src/components/react/*"],
      "@/assets*": ["src/assets/*"],
      "@/styles*": ["src/styles/*"],
      "@/hooks*": ["src/hooks/*"],
    }
  }
}